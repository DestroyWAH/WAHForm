function SwitDiv(vDiv)
{
	if (document.condition.div_status.value =="H"){
		showDiv(vDiv);
		document.condition.div_status.value = "S";
	}else{
		hideDiv(vDiv);
		document.condition.div_status.value = "H";
	}
}

function hideDiv(vDiv)
{ 
	if (document.getElementById)
	{ // DOM3 = IE5, NS6 
		document.getElementById(vDiv).style.visibility = 'hidden'; 
	} 
	else
	{ 
		if (document.layers)
		{ // Netscape 4 
			document.hideShow.visibility = 'hidden'; 
		} 
		else
		{ // IE 4 
			document.all.hideShow.style.visibility = 'hidden'; 
		} 
	} 
} 

function showDiv(vDiv)
{ 
	if (document.getElementById)
	{ // DOM3 = IE5, NS6 
		document.getElementById(vDiv).style.visibility = 'visible'; 
	} 
	else
	{ 
		if (document.layers)
		{ // Netscape 4 
			document.hideShow.visibility = 'visible'; 
		} 
		else
		{ // IE 4 
			document.all.hideShow.style.visibility = 'visible'; 
		} 
	} 
} 


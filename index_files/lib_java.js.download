		return false;
	else if (!f_checkinteger(sYear)) 
		return false;
	else if (!f_checkrange(sYear, 0, 9999)) 
		return false;
	else if (!f_checkinteger(sDay)) 
		return false;
	else if (!f_checkday(sYear, sMonth, sDay))
		return false;
	else
		return true;
}



$().ready(function() {
	var textboxes_visible = $("input:text:visible,select:visible,textarea:visible,input:checkbox:visible,input:radio:visible");

	if ($.browser.mozilla) 
	 {
		$(textboxes_visible).live("keypresss",checkForEnter);		 
	 }
	 else
	 {
		$(textboxes_visible).live("keydown",checkForEnter);		
	 }
	 
	//ทำให้รู้ตำแหน่งเวลาเลื่อนไปแต่ละ input box
	$("input,textarea,select").live("focus",function() {
		$(this).addClass("curFocus");
	});
	
	$("input,textarea,select").live("blur",function() {
		$(this).removeClass("curFocus");
	});

	// ให้ inpurt box ช่องกรอกข้อมูลขึ้นแทปเพื่อให้สามารถลบแก้ไขได้
	$(":text").live("focus",function() {
		$(this).select();
	});


	//ให้ radio สามารถยกเลิกหลังจากกดปุ่มไปแล้วได้
	var radioChecked;
	$(':radio').live('mousedown', function() {
		radioChecked = $(this).attr('checked');
	});
	$(':radio').live('click', function() {
		if (radioChecked) {
			$(this).attr('checked', false);
		} else {
			$(this).attr('checked', true);
		}
	}); 		 
});



function checkForEnter (event) 
{
    if (event.keyCode == 13) 
	{
		if ($("div.autosuggest:visible").length !=0){
			return event.keyCode;
		}	
		var textboxes = $("input:text:visible:not('.text-readonly-box,.text_readonly,.id-readonly-box,.text-readonly-right-box,.text-readonly-center-box,.text-center-readonly-box,.text-readonly-right-box,.text-readonly-box,.text-readonly-center-box,.id-readonly-box,.text-center-readonly-box'),select:visible,textarea:visible,input:checkbox:visible,input:radio:visible");
        currentBoxNumber = textboxes.index(this);	
		if (currentBoxNumber <  0){ return false;}
		if (textboxes[currentBoxNumber].type=='textarea'){	
			return event.keyCode;
		}
		 var i = 1;
		 if (textboxes[currentBoxNumber + 1] == null){ return false;}
		nextBox = textboxes[currentBoxNumber + 1];
		nextBox.focus();
		event.preventDefault();
		return false;
    }
}

